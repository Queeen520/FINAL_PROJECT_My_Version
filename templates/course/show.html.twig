{% extends 'base.html.twig' %} {% block title %}Course{% endblock %} {% block
body %}
<div class="container">
  <h1>Overview</h1>

  <table class="table w-75">
    <tbody>
      <tr>
        <th>Course:</th>
        <td>{{ course.fkCourseCategory.name }}</td>
      </tr>
      <tr>
        <th>StartTime</th>
        <td>
          {{ course.startTime ? course.startTime|date('Y-m-d H:i:s') : '' }}
        </td>
      </tr>
      <tr>
        <th>EndTime</th>
        <td>{{ course.endTime ? course.endTime|date('Y-m-d H:i:s') : '' }}</td>
      </tr>
    </tbody>
  </table>
  <a class="btn btn-outline-danger" href="{{ path('app_course_index') }}"
    >Go Back</a
  >
</div>

{% endblock %}
